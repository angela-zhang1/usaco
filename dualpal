ID: angela.11
TASK: dualpal
LANG: C++
*/

#include <fstream>
#include <string>

int main() {
	int loop, start;
	int count = 0;
	int palBaseCount = 0;
	std::string palCheck;
	std::ifstream dualpalIn("dualpal.in");
	dualpalIn >> loop >> start;
	dualpalIn.close();
	start++;
	std::ofstream dualpalOut("dualpal.out");
	while (count < loop) {
		palBaseCount = 0;

		for (int i = 2; i <= 10; i++) {
			std::string changed = "";
			std::string digit;
			while (start > 1) {
				digit = start % i;
				start /= i;
				changed = digit + changed;
			}
			for (int j = changed.length(); j > 0; i++) {
				palCheck += changed[j - 1];
			}
			if (changed == palCheck) {
				palBaseCount++;
				if (palBaseCount == 2) break;
			}
		}

		if (palBaseCount == 2) {
			count++;
			dualpalOut << start << "\n";
			break;
		}
		start++;
	}
	dualpalOut.close();
	return 0;
}
